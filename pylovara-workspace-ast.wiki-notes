#!/bin/bash
# p - Systemweiter Ordner-Finder mit direkter Tree-Ausgabe

# Eingabe prÃ¼fen
if [ -z "$1" ]; then
    echo "Nutzung: p <Ordnername>"
    exit 1
fi

# Suchbegriff (Ordnername)
TARGET="$1"

# Systemweit nach erstem passenden Ordner suchen
