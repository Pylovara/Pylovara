    # SyntaxprÃ¼fung
    if ! bash -n "$FILE"; then
        echo "Syntaxfehler in Skript gefunden!"
        echo "Versuche automatische Korrektur..."
        sed -i 's/\<fi\>/fi/g' "$FILE"
        echo "Korrektur abgeschlossen."
    fi
}


